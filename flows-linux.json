        "z": "10cee346a54fd2e6",
        "name": "fcolor",
        "func": "var result = msg.payload;\n\nresult = result.toLowerCase().trim();\n\nif (result === \"anger\") {\n    msg.payload = \"Blue\";\n} else if (result === \"sad\") {\n    msg.payload = \"Pale Yellow\";\n} else if (result === \"happy\") {\n    msg.payload = \"Light Yellow\";\n} else if (result === \"neutral\") {\n    msg.payload = \"Beige White\";\n} else {\n    msg.payload = \"\" + result + \" was not yet supported. Please use the correct model.\";\n}\nreturn msg;\n",
